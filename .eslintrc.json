{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "space-in-parens": 0,
        "linebreak-style": 0,
        "template-curly-spacing": 0
    }
}
